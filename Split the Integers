import java.util.*;
import java.util.*;

class Main{
    public static void main(String []args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int dig;
        while(n!=0) {
            dig = n % 10;
            System.out.println(dig);
            n = n / 10;
        }
    }
}
